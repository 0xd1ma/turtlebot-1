<launch>
  <param name="robot_description" command="$(find xacro)/xacro.py '$(find turtlebot_urdf)/urdf/turtlebot.urdf.xacro'" />

  <node pkg="diagnostic_aggregator" type="aggregator_node" name="diagnostic_aggregator" >
    <!-- Load the file you made above -->
    <rosparam command="load" file="$(find turtlebot_dashboard)/config/diagnostics.yaml" />
  </node>


  <node pkg="robot_state_publisher" type="state_publisher" name="robot_state_publisher" output="screen">
    <param name="publish_frequency" type="double" value="30.0" />
    <!--param name="tf_prefix" type="string" value="" /-->
  </node>

  <!-- Turtlebot Driver -->
  <node pkg="turtlebot_node" type="turtlebot_node.py" name="turtlebot_node" >
    <param name="bonus" value="false" />
  </node>

  <!--- Teleop Joystick -->
  <node pkg="turtlebot_teleop" type="turtlebot_joy" name="turtlebot_teleop">
    <remap from="cmd_vel" to="/turtlebot_node/cmd_vel"/>
    <param name="a_scale" type="double" value="1.4"/>
  </node>

  <node pkg="joy" type="joy_node" name="joystick"/>

  <!--node pkg="microstrain_3dmgx2_imu" type="imu_node" name="imu_node" output="screen">
    <param name="port" type="string" value="/dev/ttyUSB1" />
  </node--> 


</launch>
